{
  "name" : "Cryptogram Decoder",
  "version" : "0.1",
  "description" : "Decrypt Cryptogram Images",
  "background" : {  	 
	"matches": ["http://*/"],
  	"scripts": ["background.js"] },
  "permissions" : [
    "contextMenus",
    "tabs",
    "http://*/",
    "https://*/"
   ],
 	"content_security_policy": "default-src 'self'; img-src 'self' data: chrome: filesystem:",
 	"minimum_chrome_version" : "6.0.0.0",
  "icons" : {
    "16" : "icon.png"
   },
  "manifest_version": 2
}
